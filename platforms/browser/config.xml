<?xml version='1.0' encoding='utf-8'?>
<widget id="com.langfc.prod" version="1.1.07" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Lang'FC</name>
    <description>
        Fiszki - polsko-angielskie i angielsko-polskie / Flashcards - polish-english and english-polish
    </description>
    <author email="aleksander.nyczyk@gmail.com" href="http://semjs.pl">
        Aleksander Nyczyk
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="18" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
